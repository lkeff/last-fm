{
  "name": "last-fm",
  "description": "Simple, robust LastFM API client (for public data)",
  "version": "5.3.0",
  "author": {
    "name": "Feross Aboukhadijeh",
    "email": "feross@feross.org",
    "url": "https://feross.org"
  },
  "bugs": {
    "url": "https://github.com/feross/last-fm/issues"
  },
  "dependencies": {
    "run-parallel": "^1.1.10",
    "simple-get": "^4.0.0",
    "electron": "^25.4.1",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "standard": "*",
    "electron-builder": "^24.6.4"
  },
  "homepage": "https://github.com/feross/last-fm",
  "keywords": [
    "lastfm",
    "lastfm api",
    "lastfm data",
    "music",
    "music api",
    "music data",
    "last fm",
    "last fm api",
    "last fm data",
    "public data",
    "get",
    "data",
    "api",
    "last.fm",
    "last.fm api"
  ],
  "license": "MIT",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/feross/last-fm.git"
  },
  "scripts": {
    "start": "electron .",
    "dev": "electron .",
    "test": "standard && node -e \"require('./')\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.feross.lastfm",
    "productName": "LastFM Desktop",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "preload.js",
      "index.html",
      "renderer.js",
      "styles.css",
      "assets/**/*",
      "fiddle-templates/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        "nsis"
      ]
    },
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/feross"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/feross"
    },
    {
      "type": "consulting",
      "url": "https://feross.org/support"
    }
  ]
}