{
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Electron App",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
      "program": "${workspaceFolder}/main.js",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "sourceMaps": true,
      "protocol": "inspector",
      "autoAttachChildProcesses": true
    }
  ]
}